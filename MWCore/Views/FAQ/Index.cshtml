@model MWCore.Models.MWFrontEndModel
<!--/navbreadcrumb Section ends Here-->
<nav aria-label="breadcrumb" class="container mt-2">
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="@Url.Action("Index","Home")">Home</a></li>
        <li class="breadcrumb-item active">@Model.oPage.PageTitle</li>
    </ol>
</nav>
<!--/navbreadcrumb-->
<!--inner content Section Start Here-->

<div class="inner-page-wrap ">
    <div class="container">
        <div class="inner-page-hold">
            <h1 class="inner-page-title">@Model.oPage.PageTitle</h1>
        </div>
    </div>

    <section class="inner-content faq-main  ">
        <div class="container">
            <div class="faq">
                <div class="row">

                    <div class="col-lg-6 col-md-12 col-sm-12">
                        <div class="accordion" id="accordionExample">
                            @foreach (var item in Model.lstFAQs)
                            {
                                <div class="card">
                                    <div class="card-header" id="heading_@item.FAQID">
                                        <h2 class="mb-0">
                                            <button class="btn btn-link btn-block text-left" type="button" data-toggle="collapse"
                                                    data-target="#collapse_@item.FAQID" aria-expanded="true" aria-controls="collapse_@item.FAQID">
                                                @item.FAQQuetion <i class="fas fa-chevron-circle-down"></i>
                                            </button>
                                        </h2>
                                    </div>

                                    <div id="collapse_@item.FAQID" class="collapse" aria-labelledby="heading_@item.FAQID"
                                         data-parent="#accordionExample">
                                        <div class="card-body">
                                            @item.FAQAnswer
                                        </div>
                                    </div>
                                </div>
                            }
                        </div>


                    </div>

                    <div class="col-lg-6 col-md-6 col-sm-12">
                        <div class="faq-image">
                            <img src="@Url.Content("~/assets/images/uploads/")@Model.GetKeyValue("FAQ_Image")" alt="">
                        </div>
                    </div>

                </div>

            </div>






        </div>
    </section>
</div>

<!--/inner content Section ends Here-->